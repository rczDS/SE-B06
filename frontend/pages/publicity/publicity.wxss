page {
  height: 100%;
  width: 500rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.table {
  border: 0px solid darkgray;
  font-size: small;
 }
 .tr {
  display: flex;
  width: 200%;
  justify-content: center;
  height: 3rem;
  align-items: center;
 }
 .td {
  width: 40%;
  justify-content: center;
  text-align: center;
 }
 .bg-w{
  background: snow;
 }
 .bg-g{
  background: #E6F3F9;
 }
 .th {
  width: 50%;
  justify-content: center;
  background: #000000;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
 }
 button{
   width: 50%;
   height: 50%;
   font-size: small;
 }
 .default{
   width: 200%;
 }